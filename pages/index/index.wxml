<view class="page">
    <view class="page__bd">
        <!-- Plane Track -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AR+内容</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">用AR平面跟踪显示玩具机器人。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_world_track/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- Measure -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AR+效率</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">用AR测量平面上物体的长度。注：V1版测量结果不准确。如果要准确的测量，请使用V2版小程序AR。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_measure/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- Spatial Audio-->
        <view class="weui-panel">
            <view class="weui-panel__hd">AR+游戏</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">用AR跟随用户位置变化的空间化音频。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_spatial_audio/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- Image Classify -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AR+AI目标分类</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">用AI检测图像中的物体，AR将物体名称固定地显示在物体上。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_image_classify/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- Face Detect -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AI人脸检测（旧版）</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">试戴虚拟眼镜，使用wx.faceDetect()。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_face_detect/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                        <navigator url="/package_face_detect/pages/photo/photo" class="marginTop10">
                            <button class="weui-btn" type="default">拍照</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- One-Shot tracker -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AI单样本检测</view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">利用深度学习检测现实环境中目标物体的位置。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_oneshot_tracker/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- Face Detect v2 -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AI人脸检测（新版） <label class="color-red">New</label></view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">试戴虚拟眼镜，使用wx.createVKSession()。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_face_detect_v2/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                        <navigator url="/package_face_detect_v2/pages/photo/photo" class="marginTop10">
                            <button class="weui-btn" type="default">拍照</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- OCR Detect -->
        <view class="weui-panel">
            <view class="weui-panel__hd">AI文本检测 <label class="color-red">New</label></view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">检测图片和摄像头画面中的文字。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_text_detect/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                        <navigator url="/package_text_detect/pages/photo/photo" class="marginTop10">
                            <button class="weui-btn" type="default">拍照</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>

        <!-- Body Detect
        <view class="weui-panel">
            <view class="weui-panel__hd">AI人体姿态检测 <label class="color-red">New</label></view>
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__desc">检测人体姿态。</view>
                    <view class="weui-media-box__info">
                        <navigator url="/package_body_detect/pages/camera/camera" class="marginTop10">
                            <button class="weui-btn" type="primary">试一试</button>
                        </navigator>
                        <navigator url="/package_body_detect/pages/photo/photo" class="marginTop10">
                            <button class="weui-btn" type="default">拍照</button>
                        </navigator>
                    </view>
                </view>
            </view>
        </view>
        -->
    </view>
</view>